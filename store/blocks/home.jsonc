{
  "store.home": {
    "blocks": [
      "responsive-layout.desktop#desktop-home",
      "responsive-layout.mobile#mobile-home"
    ]
  },
  "responsive-layout.desktop#desktop-home": {
    "children": [
      "slider-layout#home",
      "flex-layout.row#categorias-1",
      "flex-layout.row#categorias-2",
      "shelf#Recomendados",
      "flex-layout.row#categorias-1",
      "flex-layout.row#categorias-2",
      "shelf#Recomendados",
      "flex-layout.row#categorias-1",
      "flex-layout.row#categorias-2",
      "shelf#Recomendados",
      "flex-layout.row#categorias-1",
      "flex-layout.row#categorias-2",
      "shelf#Recomendados",
      "flex-layout.row#nosotros"
    ]
  },
  "responsive-layout.mobile#mobile-home": {
    "children": [
      "slider-layout#home-mobile",
      "flex-layout.row#categorias-1",
      "flex-layout.row#categorias-2",
      "flex-layout.row#nosotros-mobile"
    ]
  },
  "rich-text#Not-combos-titulo":{
    "props":{
        "text": "Titulo",
        "textPosition": "CENTER",
        "blockClass": "texto-not-combos-titulo"
    }
  },
  "flex-layout.row#categorias-1": {
    "children": [
      "image#categoria-top-1",
      "image#categoria-top-2"
    ],
    "props":{
      "blockClass": "imagenes-categorias"
    }
  },
  "image#categoria-top-1": {
