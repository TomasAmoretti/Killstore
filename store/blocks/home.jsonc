  /* Nosotros */
  "flex-layout.row#nosotros":{
    "children": [
      "image#nosotros",
      "flex-layout.col#texto-nosotros"
    ],
    "props":{
      "blockClass": "nosotros",
      "preventHorizontalStretch": true
    }
  },
  "image#nosotros": {
    "props": {
      "src": "https://dummyimage.com/700x380/000/fff",
      "maxWidth": 610,
      "link":{
        "url":"#"
      },
      "blockClass": "imagen-nosotros"
    }
  },
  "flex-layout.col#texto-nosotros":{
    "props": {
      "verticalAlign": "middle",
      "blockClass": "col-nosotros"
    },
    "children":[
      "rich-text#nosotros",
      "menu-item#Nosotros"
    ]
  },
  "rich-text#nosotros":{
    "props": {
      "text": "Texto...",
      "textPosition": "CENTER",
      "blockClass": "texto-nosotros"
    }
  },
  /* Mobile Nosotros */
  "flex-layout.row#nosotros-mobile":{
    "children": [
      "image#nosotros-mobile",
      "flex-layout.col#texto-nosotros-mobile"
